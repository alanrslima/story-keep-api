{
  "name": "story-keep-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch-all",
    "test:integration": "jest -c jest.config.integration.js --detectOpenHandles --forceExit --silent",
    "build": "rimraf dist && tsc",
    "start": "node ./dist/src/main/server.js",
    "dev": "tsx watch ./src/main/server.ts",
    "migration:create": "typeorm migration:create ./src/module/common/infra/data-source/mysql-migrations/$npm_config_name",
    "migration:run": "typeorm -d ./src/module/common/infra/data-source/mysql-data-source.ts -- migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/module/common/infra/data-source/typeorm-data-source.ts",
    "stripe:listener": "stripe listen --forward-to localhost:3000/api/memory/stripe-webhook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.772.0",
    "@aws-sdk/s3-request-presigner": "^3.772.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "openid-client": "^5.6.5",
    "rimraf": "^6.0.1",
    "stripe": "^18.0.0",
    "tsx": "^4.19.3",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0"
  }
}
